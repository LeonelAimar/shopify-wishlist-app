{
    "name": "shopify-wishlist-app",
    "version": "1.0.0",
    "description": "A Shopify Wishlist App that can be used seamlessly with the [Shopify Wishlist](https://github.com/dlerm/shopify-wishlist) theme files.",
    "engines": {
        "node": "16.x"
    },
    "main": "index.js",
    "scripts": {
        "watch": "tsc -w",
        "init": "cross-env NODE_ENV=production node dist/index.js",
        "start": "tsc && cross-env NODE_ENV=development nodemon dist/index.js"
    },
    "type": "module",
    "dependencies": {
        "@shopify/shopify-api": "^3.0.1",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^10.0.0",
        "express": "^4.17.3",
        "shopify-api-node": "^3.9.0"
    },
    "repository": {
        "type": "git",
        "url": ""
    },
    "keywords": [
        "shopify",
        "javascript",
        "node",
        "express",
        "wishlist"
    ],
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "nodemon": "^2.0.14",
        "typescript": "^4.6.3"
    }
}
